{
    "source": {
      "include": ["controller/", "models/", "utils/"],
      "exclude": ["node_modules/"]
    },
    "plugins": [
      "plugins/markdown",
      "plugins/summarize"
    ],
    "opts": {
      "destination": "docs/",
      "recurse": true
    },
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "useLongnameInNav": false,
      "showInheritedInNav": true
    }
  }
  